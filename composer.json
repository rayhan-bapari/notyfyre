{
	"name": "rayhan-bapari/notyfyre",
	"description": "A beautiful toast notification package for Laravel with modern design and title-only notifications",
	"type": "library",
	"license": "MIT",
	"keywords": [
		"laravel",
		"notifications",
		"toast",
		"alerts",
		"flash-messages",
		"notyfyre"
	],
	"authors": [
		{
			"name": "Rayhan Bapari",
			"email": "mdrayhanbapari02@gmail.com"
		}
	],
	"require": {
		"php": "^8.0|^8.1|^8.2|^8.3|^8.4",
		"laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "vendor/package": "^2.0"
	},
	"require-dev": {
		"orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
		"phpunit/phpunit": "^9.0|^10.0|^11.0"
	},
	"autoload": {
		"psr-4": {
			"RayhanBapari\\Notyfyre\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"RayhanBapari\\Notyfyre\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"RayhanBapari\\Notyfyre\\NotyfyreServiceProvider"
			],
			"aliases": {
				"Notyfyre": "RayhanBapari\\Notyfyre\\Facades\\Notyfyre"
			}
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
